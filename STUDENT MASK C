#include <stdio.h>

int main() {
    int mark;

    printf("enter student mark:");
    scanf("%d", &mark);

   if (mark >=80){
       printf("grade:A\n");
}   else if(mark>= 70){
      printf("grade:B\n");
}   else if (mark >=60) {
      printf("grade:C\n");
}  else if(mark>=50){
     printf("grade:D\n")
} else{
     printf("grade:F\n");
}

   return 0;
}
